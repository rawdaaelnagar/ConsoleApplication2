#include <iostream>
using namespace std;
int convert(int n){
    if(n==0){
        return 0;}
int binaryy[32];
long long t;
int i=0;
cin>>t;
    int sumb=32;
while (sumb> 0)
    {
        binaryy[i] = t % 2;
        t = t/ 2;
        i++;
        sumb-=1;
    }
    for (int j = i - 1; j >= 0; j--){
            if(binaryy[j]==1){
                    for(int k=j;k>=0;k--){
                    cout << binaryy[k];}
                    j=0;}
}
    cout<<endl;
    return convert(n-1);
}
int main()
{
    int n;
    cin>>n ;
    convert(n);
    return 0;}
