#include <iostream>
#include <vector>
using namespace std;

void evenI(vector<long long> arr, int index) {
	if (index < 0) {
		return;
	}
	if (index % 2 == 0) {
		cout << arr[index] << " ";
	}
	evenI(arr, index - 1);
}

int main() {
	int n;
	cin >> n;

	vector<long long> arr(n);
	for (int i = 0; i < n; i++) {
		cin >> arr[i];
	}

	evenI(arr, n - 1);
	return 0;
}
