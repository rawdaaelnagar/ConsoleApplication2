#include <iostream>
#include <vector>
using namespace std;

long long sumArray(vector<long long> arr, int index) {
    if (index == 0) {
        return arr[0];
    }
    
    return arr[index] + sumArray(arr, index - 1);
}

int main() {
    int n;
    cin >> n;
    
    vector<long long> arr(n);
    for (int i = 0; i < n; i++) {
        cin >> arr[i];
    }
    
    cout << sumArray(arr, n - 1) << endl;
    return 0;
}
